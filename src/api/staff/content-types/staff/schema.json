{
  "kind": "collectionType",
  "collectionName": "staffs",
  "info": {
    "singularName": "staff",
    "pluralName": "staffs",
    "displayName": "Staff"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "default": "John Smith",
      "required": true
    },
    "role": {
      "type": "string"
    },
    "headshot": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "bio": {
      "type": "blocks"
    },
    "stories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::story.story",
      "inversedBy": "staff"
    },
    "shows": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::show.show",
      "mappedBy": "ep"
    }
  }
}
