{
  "kind": "collectionType",
  "collectionName": "shows",
  "info": {
    "singularName": "show",
    "pluralName": "shows",
    "displayName": "Show"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "default": "Northwestern News Report",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "heroVideoUrl": {
      "type": "string"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "stories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::story.story",
      "mappedBy": "show"
    },
    "ep": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::staff.staff",
      "inversedBy": "shows"
    },
    "series": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::series.series",
      "inversedBy": "shows"
    }
  }
}
